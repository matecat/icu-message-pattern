{
    "name": "matecat/icu-message-pattern",
    "description": "A PHP port of ICU4J's MessagePattern parser that parses ICU MessageFormat patterns into an inspectable AST and token stream so you can analyze, validate, and implement MessageFormat-compatible formatters in PHP.",
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Matecat\\ICU\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Domenico Lupinetti",
            "email": "ostico@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "phpunit/php-code-coverage": "^12",
        "phpstan/phpstan": "@stable",
        "ext-xdebug": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Matecat\\ICU\\Tests\\": "tests/Matecat/Tests/ICU"
        }
    }
}